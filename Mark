#include<stdio.h>
int main()
{
int math,chem,phy,total,totalmathphy;
pr intf ("enter marks in math:");
scanf("%d",&math);
printf ("enter marks in chem:");
scanf("%d",&chem);
printf ("enter marks in phy:");
scanf("%d",&phy);
total=math+chem+phy; 
totalmathphy=math+phy;
if(math>=65 && chem>=50 && phy>=55 && (total>=180||totalmathphy>=140))
{
printf("The candidate is eligible for admission\n");
}else{
printf("The candidate is not eligible for admission\n");
}
return 0;
}
